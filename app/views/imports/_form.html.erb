<%= form_with(model: import, method: :post, local: true) do |form| %>
  <div class="field">
   <%= form.label :select_company%>
   <%= select_tag "company_id", options_from_collection_for_select(@company, "id", "name") %>
   </div>
   <br/>
   <div class="field">
   	<%= form_tag 'imports_path', multipart: true do %>
  <label for="file">File to Upload</label> <%= file_field_tag "file" %>
  <%= submit_tag %>
<% end %>
  </div>
  <br />
<div class="field">
  <div class="actions">
    <%= form.submit %>
  </div>
</div>
<% end %>