<p id="notice"><%= notice %></p>
  <div>
    <b>select company:</b>
      <%= select_tag "company_id", options_from_collection_for_select(@company, "id", "name") %>
  </div>
  <div>
    <b>select month:</b>
      <%= select_month(Date.today) %>
  </div>
  <div>
    <b>upload file:</b>
      <%= form_with(url: imports_path, method: :post, multipart: :true) do |f| %>
        <%= f.file_field :file %>
        <%= f.submit "Import File" %>
      <% end %>
  </div>